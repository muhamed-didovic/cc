{
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "ext-mbstring": "*",
    "symfony/console": "*",
    "guzzlehttp/guzzle": "*",
    "symfony/dom-crawler": "~2.6",
    "symfony/css-selector": "~3.0@dev",
    "vlucas/phpdotenv": "^2.3",
    "tightenco/collect": "^5.4",
    "league/flysystem": "~2.1",
    "gabrielelana/byte-units": "^0.5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    },
    "files": ["src/Library/Helper.php"]
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "scripts": {
    "fix" : "php vendor/bin/php-cs-fixer fix --using-cache=no"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
