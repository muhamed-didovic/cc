{
  "require": {
    "php": "^8.0.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "symfony/console": "^6.0.1",
    "guzzlehttp/guzzle": "*",
    "symfony/dom-crawler": "^6.0.1",
    "symfony/css-selector": "^6.0.1",
    "vlucas/phpdotenv": "^5.4.1",
    "tightenco/collect": "^8.76.1",
    "league/flysystem": "~2.3.2",
    "gabrielelana/byte-units": "^0.5.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    },
    "files": ["src/Library/Helper.php"]
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4.0"
  },
  "scripts": {
    "fix" : "php vendor/bin/php-cs-fixer fix --using-cache=no"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
